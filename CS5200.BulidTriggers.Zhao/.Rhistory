library(RSQLite)
fpath = ""
dbfile = "committeeDB.sqlite"
# if database file already exists, we connect to it, otherwise
# we create a new database
dbcon <- dbConnect(RSQLite::SQLite(), paste0(fpath,dbfile))
dbExecute(dbcon, "PRAGMA foreign_keys = ON")
dbDisconnect(dbcon)
